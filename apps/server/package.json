{
  "name": "@ai-base/server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "sync-prisma": "prisma generate",
    "postinstall": "pnpm sync-prisma",
    "dev": "pnpm build-dev && node dist/server.js",
    "start": "pnpm build-dev && node dist/server.js",
    "build-dev": "tsc --noEmit && esbuild --bundle --sourcemap=inline --define:process.env.NODE_ENV=\\\"development\\\" --platform=node --target=node16 --outdir=dist \"src/server.ts\"",
    "build": "tsc --noEmit && esbuild --bundle --define:process.env.NODE_ENV=\\\"production\\\" --platform=node --target=node16 --outdir=dist --minify --sourcemap \"src/server.ts\"",
    "db:dev": "prisma migrate dev",
    "dbseed": "prisma db seed",
    "deploy:db": "prisma migrate deploy && pnpm dbseed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-serve-static-core": "0.1.1",
    "jsonwebtoken": "^9.0.2",
    "md5": "^2.3.0",
    "socket.io": "^4.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.41",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.8.10",
    "esbuild": "^0.19.5",
    "prisma": "^5.5.2",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
